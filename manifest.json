{
  "description": "FUCKYOUb",
  "manifest_version": 2,
  "name": "HIHIHIHI",
  "version": "1.0",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/beastify",
  "icons": {
    "48": "icons/beasts-48.png"
  },
  "sidebar_action": {
    "browser_style": true,
    "default_title": "CHUNGUS PANEL",
    "default_panel": "panel.html",
    "default_icon": {
      "19": "icons/window19.png",
      "38": "icons/window38.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_scripts/client.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "downloads",
    "downloads.open",
    "storage",
    "<all_urls>",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/beasts-32.png",
    "theme_icons": [
      {
        "light": "icons/beasts-32-light.png",
        "dark": "icons/beasts-32.png",
        "size": 32
      }
    ],
    "default_title": "DELUXE"
  }
}
